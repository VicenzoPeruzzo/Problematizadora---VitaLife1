// JavaScript Example: Reading Entities
// Filterable fields: title, message, type, date, is_read, user_email
async function fetchReminderEntities() {
    const response = await fetch(`https://app.base44.com/api/apps/68e2be3b93bbccf5dac77879/entities/Reminder`, {
        headers: {
            'api_key': '7efa358b7ea646b683b1dbfa350736f0', // or use await User.me() to get the API key
            'Content-Type': 'application/json'
        }
    });
    const data = await response.json();
    console.log(data);
}

// JavaScript Example: Updating an Entity
// Filterable fields: title, message, type, date, is_read, user_email
async function updateReminderEntity(entityId, updateData) {
    const response = await fetch(`https://app.base44.com/api/apps/68e2be3b93bbccf5dac77879/entities/Reminder/${entityId}`, {
        method: 'PUT',
        headers: {
            'api_key': '7efa358b7ea646b683b1dbfa350736f0', // or use await User.me() to get the API key
            'Content-Type': 'application/json'
        },
        body: JSON.stringify(updateData)
    });
    const data = await response.json();
    console.log(data);
}
